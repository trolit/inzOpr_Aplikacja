@model IEnumerable<crud.Models.Dokumentacja>

@{
    <!-- tytul strony -->
    ViewBag.Title = "Tytul strony";
}

<head>
    <!-- zalaczenie biblioteki globalnej style.css -->
    <link rel="Stylesheet" href="~/Content/style.css" type="text/css" />
</head>

<div class="naglowekStrony">
    Dokumentacja
</div>

<p>
    @Html.ActionLink("Create New", "CreateDokumentacja")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nazwaDokumentu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.czyUkonczony)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nazwaDokumentu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.czyUkonczony)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditDokumentacja", new { id = item.idDokumentu }) |
            @Html.ActionLink("Details", "DetailsDokumentacja", new { id = item.idDokumentu }) |
            @Html.ActionLink("Delete", "DeleteDokumentacja", new { id = item.idDokumentu })
        </td>
    </tr>
    }

</table>
